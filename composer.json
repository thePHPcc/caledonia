{
    "config": {
        "platform": {
            "php": "8.4.4"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": ">=8.4",
        "ext-mysqli": "*"
    },
    "require-dev": {
        "sebastian/csv-parser": "^2.0.0",
        "sebastian/mysqli-wrapper": "^2.0.0",
        "sebastian/uuid": "^1.0.2"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/src/"
        ]
    }
}
